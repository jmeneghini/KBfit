<KBFit>
    <Initialize>
        <ProjectName>LLSingleChannelSWave</ProjectName>
        <LogFile>LLSingleChannelSWave.log</LogFile>
        <EchoXML/>
        <MCSamplingInfo>
            <Bootstrapper>
                <NumberResamplings>2000</NumberResamplings>
                <Seed>3103</Seed>
                <BootSkip>0</BootSkip>
            </Bootstrapper>
        </MCSamplingInfo>
        <MCBinsInfo>
            <MCEnsembleInfo>cls21_s64_t128_D200</MCEnsembleInfo>
            <TweakEnsemble>
                <Ommissions>2000</Ommissions>
                <Rebin>10</Rebin>
            </TweakEnsemble>
        </MCBinsInfo>
    </Initialize>

    <TaskSequence>
        <Task>
            <Action>DoSingleChannel</Action>

            <OutputStub>LLSingleChannelSWave</OutputStub>
<!--            <OutputMode>full</OutputMode>-->
            <OutputMode>resampled</OutputMode>
            <SamplingsOutputStub>LLSingleChannelSWave</SamplingsOutputStub>


            <DecayChannels>
                <DecayChannelInfo>
                    <Particle1Name>lambda</Particle1Name>
                    <Spin1TimesTwo>1</Spin1TimesTwo>
                    <Identical/>
                </DecayChannelInfo>
            </DecayChannels>


            <DefaultEnergyFormat>reference_ratio</DefaultEnergyFormat>

            <MCEnsembleParameters>
                <MCEnsembleInfo>cls21_s64_t128_D200</MCEnsembleInfo>
                <ReferenceMassTimeSpacingProduct>
                    <MCObs>L(0)_elab 0</MCObs>
                </ReferenceMassTimeSpacingProduct>
                <!--                <LatticeAnisotropy>-->
                <!--                    <MCObs>xi_pion 0</MCObs>-->
                <!--                </LatticeAnisotropy>-->
                <ParticleMass>
                    <Name>lambda</Name>
                    <FixedValue>1.0</FixedValue>
                </ParticleMass>
            </MCEnsembleParameters>

            <KBBlock>
                <MCEnsembleInfo>cls21_s64_t128_D200</MCEnsembleInfo>
                <BoxQuantization>
                    <TotalMomentumRay>ar</TotalMomentumRay>
                    <TotalMomentumIntSquared>0</TotalMomentumIntSquared>
                    <LGIrrep>A1g</LGIrrep>
                    <LmaxValues>0</LmaxValues>
                </BoxQuantization>
                <LabFrameEnergy>
                    <MCObs>isosinglet_S=-2_A1g_PSQ=0_elab_0_ref 0</MCObs>
                </LabFrameEnergy>
                <LabFrameEnergy>
                    <MCObs>isosinglet_S=-2_A1g_PSQ=0_elab_1_ref 0</MCObs>
                </LabFrameEnergy>
                <LabFrameEnergy>
                    <MCObs>isosinglet_S=-2_A1g_PSQ=0_elab_2_ref 0</MCObs>
                </LabFrameEnergy>
                <LabFrameEnergy>
                    <MCObs>isosinglet_S=-2_A1g_PSQ=0_elab_3_ref 0</MCObs>
                </LabFrameEnergy>
                <LabFrameEnergy>
                    <MCObs>isosinglet_S=-2_A1g_PSQ=0_elab_4_ref 0</MCObs>
                </LabFrameEnergy>
                <LabFrameEnergy>
                    <MCObs>isosinglet_S=-2_A1g_PSQ=0_elab_5_ref 0</MCObs>
                </LabFrameEnergy>
                <LabFrameEnergy>
                    <MCObs>isosinglet_S=-2_A1g_PSQ=0_elab_6_ref 0</MCObs>
                </LabFrameEnergy>
                <LabFrameEnergy>
                    <MCObs>isosinglet_S=-2_A1g_PSQ=0_elab_7_ref 0</MCObs>
                </LabFrameEnergy>
                <LabFrameEnergy>
                    <MCObs>isosinglet_S=-2_A1g_PSQ=0_elab_8_ref 0</MCObs>
                </LabFrameEnergy>
                <LabFrameEnergy>
                    <MCObs>isosinglet_S=-2_A1g_PSQ=0_elab_9_ref 0</MCObs>
                </LabFrameEnergy>

            </KBBlock>


            <KBObservables>
                <MCSamplingInfo>
                    <Bootstrapper>
                        <NumberResamplings>2000</NumberResamplings>
                        <Seed>3103</Seed>
                        <BootSkip>0</BootSkip>
                    </Bootstrapper>
                </MCSamplingInfo>
                <MCBinsInfo>
                    <MCEnsembleInfo>cls21_s64_t128_D200</MCEnsembleInfo>
                    <TweakEnsemble>
                        <Ommissions>2000</Ommissions>
                        <Rebin>10</Rebin>
                    </TweakEnsemble>
                </MCBinsInfo>
                <Verbose />
                <SamplingData>
                    <FileName>/home/john/Documents/LatticeQCD/spectrums/ensemble_based/D200/isosinglet_doublystrange/analysis/3fit_spectrum/data/samples/fit_spectrum_levels_sigmond-6-9-25-ratio-refmass-lambda-Nbin10-SP-4tN-4t0-12tD_B-samplings.hdf5[/samplings]</FileName>
