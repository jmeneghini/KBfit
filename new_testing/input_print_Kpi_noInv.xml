<KBFit>
   <Initialize>
     <ProjectName>KPiPrint</ProjectName>
     <LogFile>Kpi_print1.log</LogFile>
     <EchoXML/>
     <MCSamplingInfo>
       <Bootstrapper>
          <NumberResamplings>1000</NumberResamplings>
          <Seed>6754</Seed>
          <BootSkip>0</BootSkip>
       </Bootstrapper>
     </MCSamplingInfo>
   </Initialize>

   <TaskSequence>
      <Task>
      <Action>DoPrint</Action>

      <!-- <OutputStub>KPiPrint_Kinv</OutputStub> -->
      <OutputStub>KPiPrint_CayleyImag</OutputStub>
      <OutputMode>full</OutputMode>

        <OmegaMu>30.0</OmegaMu>

        <PrintEigenvalues>
<!--            <EigenvalueRegularizingInfo>-->
<!--              <InScalar>1.0</InScalar>-->
<!--              <OutScalar>5.0</OutScalar>-->
<!--            </EigenvalueRegularizingInfo>-->
        </PrintEigenvalues>



        <KtildeMatrix>
          <Element>
            <KElementInfo>
              <JTimesTwo>0</JTimesTwo>
              <KIndex>L(0) 2S(0) chan(0)</KIndex>
              <KIndex>L(0) 2S(0) chan(0)</KIndex>
            </KElementInfo>
            <FitForm>
              <Polynomial>
                <Powers>0 2</Powers>
              </Polynomial>
            </FitForm> <!-- quad fn of Ecm for l=0 -->
          </Element>
          <Element>
            <KElementInfo>
              <JTimesTwo>2</JTimesTwo>
              <KIndex>L(1) 2S(0) chan(0)</KIndex>
              <KIndex>L(1) 2S(0) chan(0)</KIndex>
            </KElementInfo>
            <FitForm>
              <Polynomial>
                <Powers>1 3</Powers>
              </Polynomial>
            </FitForm> <!-- Breit Wigner for l=0 -->
          </Element>

          <DecayChannels>
            <DecayChannelInfo>
              <Particle1Name>kaon</Particle1Name>
              <Spin1TimesTwo>0</Spin1TimesTwo>
              <Particle2Name>pion</Particle2Name>
              <Spin2TimesTwo>0</Spin2TimesTwo>
              <IntrinsicParities>same</IntrinsicParities>
            </DecayChannelInfo>
          </DecayChannels>

          <StartingValues>
            <KFitParamInfo>
              <PolynomialTerm>
                <Power>0</Power>
                <KElementInfo>
                  <JTimesTwo>0</JTimesTwo>
                  <KIndex>L(0) 2S(0) chan(0)</KIndex>
                  <KIndex>L(0) 2S(0) chan(0)</KIndex>
                </KElementInfo>
              </PolynomialTerm>
              <StartingValue>3.9</StartingValue>
            </KFitParamInfo>

            <KFitParamInfo>
              <PolynomialTerm>
                <Power>2</Power>
                <KElementInfo>
                  <JTimesTwo>0</JTimesTwo>
                  <KIndex>L(0) 2S(0) chan(0)</KIndex>
                  <KIndex>L(0) 2S(0) chan(0)</KIndex>
                </KElementInfo>
              </PolynomialTerm>
              <StartingValue>-0.15</StartingValue>
            </KFitParamInfo>

            <KFitParamInfo>
              <PolynomialTerm>
                <Power>1</Power>
                <KElementInfo>
                  <JTimesTwo>2</JTimesTwo>
                  <KIndex>L(1) 2S(0) chan(0)</KIndex>
                  <KIndex>L(1) 2S(0) chan(0)</KIndex>
                </KElementInfo>
              </PolynomialTerm>
              <StartingValue>8.3</StartingValue>
            </KFitParamInfo>

            <KFitParamInfo>
              <PolynomialTerm>
                <Power>3</Power>
                <KElementInfo>
                  <JTimesTwo>2</JTimesTwo>
                  <KIndex>L(1) 2S(0) chan(0)</KIndex>
                  <KIndex>L(1) 2S(0) chan(0)</KIndex>
                </KElementInfo>
              </PolynomialTerm>
              <StartingValue>-0.53</StartingValue>
            </KFitParamInfo>
          </StartingValues>

        </KtildeMatrix>

<DefaultEnergyFormat>reference_ratio</DefaultEnergyFormat>

<MCEnsembleParameters>
  <MCEnsembleInfo>clover_s32_t256_ud860_s743</MCEnsembleInfo>
  <ReferenceMassTimeSpacingProduct>
    <MCObs>pion 0</MCObs>
  </ReferenceMassTimeSpacingProduct>
  <LatticeAnisotropy>
    <MCObs>xi_pion 0</MCObs>
  </LatticeAnisotropy>
  <ParticleMass>
    <Name>pion</Name>
    <FixedValue>1.0</FixedValue>
  </ParticleMass>
  <ParticleMass>
    <Name>kaon</Name>
    <MCObs>kaon_mpi 0</MCObs>
  </ParticleMass>
</MCEnsembleParameters>
